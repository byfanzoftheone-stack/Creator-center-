# Example environment variables for local development / Railway
# Copy to .env and fill values locally when testing

# PostgreSQL connection string (Railway will provide this when you add a Postgres plugin)
DATABASE_URL=postgresql://USER:PASS@HOST:PORT/DB

# JWT secret
JWT_SECRET=change-me

# Comma-separated allowed origins for CORS (frontend URL(s))
# e.g. https://your-frontend.vercel.app,https://app.example.com
CORS_ORIGINS=http://localhost:3000
